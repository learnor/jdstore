    <% if flash.any? %>
    <% user_facing_flashes.each do |key, value| %>
        <div class="container-fluid" id="notice_wrapper">
            <div class="alert alert-dismissable alert-<%= flash_class(key) %>">
                <button class="close" data-dismiss="alert">&times;</button>
                <%= value %>
            </div>
        </div>
    <% end %>
<% end %>
